	title = {Marry in or die out: Optimal inbreeding and the meaning of mediogamy},
	year = {2015},
	journal = {Handbook on Evolution and Society: Toward an Evolutionary Social Science},
	pages = {350 – 382},
	doi = {10.4324/9781315634203},
