@ARTICLE{Lorijn2023159,
	author = {Lorijn, Eleonore},
	title = {Pills, Power and Performativity: Negotiating Masculinity in the Emergence of Male Contraceptive Technology},
	year = {2023},
	journal = {Gender a Vyzkum / Gender and Research},
	volume = {24},
	number = {1},
