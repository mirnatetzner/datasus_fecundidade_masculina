@ARTICLE{Kravdal2007211,
	author = {Kravdal, Øystein},
	title = {Effects of current education on second- and third-birth rates among Norwegian women and men born in 1964: Substantive interpretations and methodological issues},
	year = {2007},
	journal = {Demographic Research},
	volume = {17},
	pages = {211 – 246},
	doi = {10.4054/DemRes.2007.17.9},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-36349024749&doi=10.4054%2fDemRes.2007.17.9&partnerID=40&md5=257ba8113e05d1504650c18a9419a875},
	affiliations = {Department of Economics, University of Oslo, 0317 Oslo, P.O. Box 1095 Blindern, Norway},
	abstract = {A variety of approaches have been employed to assess the importance of women's education for their second or third-birth rates. Some researchers have included the educational level measured at a relatively high age in their models, whereas others have included current education. A few have taken selection into account by modelling, first-, second-, and higher-order birth rates jointly, with a common unobserved factor. The corresponding education-fertility relationships among men, however, has not attracted any attention. In this study, based on Norwegian register data for the 1964 cohort, a high current educational level for a woman is found to stimulate her second- and third-birth rates. Controlling for selection through joint modelling turns out to be quife unimportant, but the results are very different if the educational level attained by age 30 is included instead of current education. It is important to be aware of such sensitivity to the specification. of education. The corresponding effects for men are also positive, but not more strongly positive than those for women. These results may suggest that we should not take for granted that women's education generally reduces fertility, and that it does so because of higher opportunity costs for the better educated. However, it is also possible that a high current educational level is linked with modest aspirations for further schooling, which would tend to stimulate subsequent fertility, that it is partly caused by some individual, family or community characteristics that also lead to high fertility, or that it even to some extent is a result of plans to have a child fairly soon. These alternative interpretations are discussed.},
	correspondence_address = {Ø. Kravdal; Department of Economics, University of Oslo, 0317 Oslo, P.O. Box 1095 Blindern, Norway; email: okravdal@econ.uio.no},
	issn = {14359871},
