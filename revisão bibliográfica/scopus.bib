	author = {Guzzo, Karen Benjamin and Hayford, Sarah R. and Lang, Vanessa Wanner},
	title = {Do Reproductive Attitudes and Knowledge Explain Race-Ethnic-Nativity Differences in Unintended Fertility?},
	year = {2020},
	journal = {Springer Series on Demographic Methods and Population Analysis},
	volume = {51},
